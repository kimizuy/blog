{
  "name": "kimizuy-blog",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8000",
    "build": "next build",
    "build:prod": "next build && node ./.next/serverless/scripts/build-rss.js",
    "start": "next start",
    "analyze": "ANALYZE=true yarn next build",
    "tsc-watch": "tsc --noEmit --watch",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint .",
    "fix:prettier": "prettier --write .",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@mdx-js/react": "^1.6.22",
    "@next/bundle-analyzer": "^10.0.8",
    "date-fns": "^2.19.0",
    "feed": "^4.2.2",
    "next": "^10.0.8",
    "preact": "^10.5.12",
    "prism-react-renderer": "^1.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-static-tweets": "^0.3.1",
    "simple-functional-loader": "^1.2.1",
    "static-tweets": "^0.3.0",
    "swr": "^0.5.3",
    "tocbot": "^4.12.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^10.0.8",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "husky": "5.1.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
